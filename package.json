{
  "name": "resume-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "build-pdf": "pandoc ./pandoc/resume.md -o ./resume.pdf --from markdown --template ./pandoc/resume.latex --pdf-engine=xelatex --pdf-engine-opt=--shell-escape --metadata-file=./src/lib/content/work-experience.yaml"
  },
  "devDependencies": {
    "@fontsource/abril-fatface": "^4.5.9",
    "@fontsource/barlow": "^4.5.9",
    "@iconify/json": "^2.2.31",
    "@iconify/svelte": "^3.1.0",
    "@rollup/plugin-yaml": "^4.0.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.10",
    "@types/color": "^3.0.3",
    "@types/markdown-it": "^12.2.3",
    "@types/marked": "^4.0.8",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "color": "^4.2.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "marked": "^4.2.12",
    "mdsvex": "^0.10.6",
    "octokit": "^2.0.14",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "slugify": "^1.6.5",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unplugin-icons": "^0.15.3",
    "vite": "^4.1.4",
    "yaml": "^2.2.1"
  },
  "type": "module"
}
